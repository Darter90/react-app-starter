{
	"name": "react-app-starter",
	"version": "1.0.0",
	"description": "React App Starter handcrafted",
	"author": "Jacopo Carosi",
	"keywords": [
		"react",
		"webpack",
		"starter"
	],
	"license": "MIT",
	"main": "index.js",
	"scripts": {
		"serve": "webpack-dev-server .",
		"format": "prettier . --write",
		"lint": "eslint . --fix --max-warnings=0",
		"test": "jest",
		"test_with_coverage": "jest --coverage",
		"clean": "rimraf coverage dist",
		"build": "rimraf coverage dist && prettier . --write && jest --coverage && webpack .",
		"build_skipTests": "rimraf coverage dist && prettier . --write && webpack ."
	},
	"devDependencies": {
		"@babel/cli": "^7.20.7",
		"@babel/core": "^7.20.12",
		"@babel/preset-env": "^7.20.2",
		"@babel/preset-react": "^7.18.6",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^14.0.0",
		"@typescript-eslint/eslint-plugin": "^5.51.0",
		"babel-loader": "^9.1.2",
		"css-loader": "^6.7.3",
		"eslint": "^8.33.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-prettier": "^8.6.0",
		"eslint-config-standard-with-typescript": "^34.0.0",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-jest": "^27.2.1",
		"eslint-plugin-jsx-a11y": "^6.7.1",
		"eslint-plugin-n": "^15.6.1",
		"eslint-plugin-promise": "^6.1.1",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"html-webpack-plugin": "^5.5.0",
		"jest": "^29.4.2",
		"jest-environment-jsdom": "^29.4.2",
		"prettier": "^2.8.7",
		"sass": "^1.58.0",
		"sass-loader": "^13.2.0",
		"style-loader": "^3.3.1",
		"webpack": "^5.75.0",
		"webpack-cli": "^5.0.1",
		"webpack-dev-server": "^4.11.1"
	},
	"dependencies": {
		"axios": "^1.3.4",
		"process": "^0.11.10",
		"dotenv": "^16.0.3",
		"prop-types": "^15.8.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"rimraf": "^4.1.2"
	}
}
